{% extends "layout.twig" %}

{% block title %}Home{% endblock %}

{% block body %}

    <h1>Welcome</h1>
    <p>Hello from a Twig template, {{ name }}!</p>

    <ul>
        {% for article in articles %}
            <li>{{ article.id }}</li>
            <li>{{ article.title }}</li>
            <li>{{ article.content }}</li>
        {% endfor %}
    </ul>

{% endblock %}
